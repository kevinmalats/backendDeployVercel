{
    "version": 2,
    "builds": [
        {
          "src": "dist/main.js", 
          "use": "@vercel/node" 
        }
      ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/vercel/output/$1",
        "methods": [
          "GET",
          "POST",
          "PUT",
          "DELETE"
      ]
      }
    ]
  }