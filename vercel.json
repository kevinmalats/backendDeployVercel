{
    "version": 2,
    "builds": [
      {
        "src": "dist/main.js",
        "use": "@vercel/node",
        "config": {
            "prisma": {
              "generate": true
            }
          }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "dist/main.js"
      }
    ],
    "installCommand": "npm install --legacy-peer-deps"
  }